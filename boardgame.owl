<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology"
     xml:base="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:ns2="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:terms1="http://open.vocab.org/terms/"
     xmlns:powder-s="http://www.w3.org/2007/05/powder-s#"
     xmlns:temporal="http://swrl.stanford.edu/ontologies/built-ins/3.3/temporal.owl"
     xmlns:boardgame="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology">
        <owl:versionIRI rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology/1.0.0"/>
        <owl:imports rdf:resource="http://dbpedia.org/ontology/"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <rdfs:comment xml:lang="it">Ontologia che definisce i giochi da tavolo ed i relativi componenti.</rdfs:comment>
        <rdfs:isDefinedBy>Gioele Masini</rdfs:isDefinedBy>
        <owl:versionInfo>1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasBase -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasBase"/>
    


    <!-- http://www.w3.org/2002/07/owl#maxQualifiedCardinality -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#maxQualifiedCardinality"/>
    


    <!-- http://xmlns.com/foaf/0.1/name -->

    <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/name"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#age -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#age">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Game"/>
        <rdfs:comment>Recommended age to play a game</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#artistOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#artistOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#involvedIn"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasArtist"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Artist"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#awarded -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#awarded">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Game"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Award"/>
        <rdfs:comment>The award of the game.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#componentOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#componentOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#creditsTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#creditsTo">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#involvedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#designerOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#designerOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#involvedIn"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasDesigner"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Designer"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#duration -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#duration">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Range"/>
        <rdfs:comment>The duration of the game.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#family -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#family">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#familyOf"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#familyOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#familyOf">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/series"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasArtist -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasArtist">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasBox -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasBox">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasComponent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Box"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasCard -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasCard">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasComponent"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Deck"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Card"/>
        <rdfs:comment>A card of the deck.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasColour -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasColour">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#GameComponent"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Colour"/>
        <rdfs:comment>The main colour of the component.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasComponent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasComponent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Box"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Game"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Set"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#GameComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasDesigner -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasDesigner">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasLanguage -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasLanguage">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#languageOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Game"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Document"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Language"/>
        <rdfs:comment>The language of the text of the board.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasPhoto -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasPhoto">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#photoOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Image"/>
        <rdfs:comment>A photo of the component.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasPublisher -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasPublisher">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#publisherOf"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#involvedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#involvedIn">
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Game"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#languageOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#languageOf">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Language"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
        <rdfs:comment>The language of the text of the book.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#madeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#madeOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#GameComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#numberOfPlayers -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#numberOfPlayers">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Range"/>
        <rdfs:comment>The number of players of the game.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#photoOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#photoOf">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Image"/>
        <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#publisherOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#publisherOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#involvedIn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#releaseDate -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#releaseDate">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Game"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Game"/>
    </owl:ObjectProperty>
    


