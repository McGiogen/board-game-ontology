                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>The maximum of the range</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasMinimum -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasMinimum"/>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasWidth -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasWidth">
