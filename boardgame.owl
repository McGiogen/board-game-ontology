        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasHeight -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasHeight">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#GameComponent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Height, in millimeters.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasMaximum -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasMaximum">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Range"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#hasMaximum"/>
