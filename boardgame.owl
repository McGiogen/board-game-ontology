    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#publicationYear -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#publicationYear">
