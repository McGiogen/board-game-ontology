    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#classic -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#classic">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Game"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>True if is a classic game</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#count -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#count">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#description -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#description">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#edition -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#edition">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#Game"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#faces -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/gioelemasini/ontologies/boardgame-ontology#faces">
