  <!-- RDFS -->
  <!-- Class definition -->
 
  <rdfs:Class rdf:ID="RolePlayingGame">
    <!--<owl:equivalentClass rdf:resource="https://www.wikidata.org/wiki/Q160738"/>-->
    <rdfs:label xml:lang="en">role playing game</rdfs:label>
    <rdfs:label xml:lang="it">gioco di ruolo</rdfs:label>
    <rdfs:comment xml:lang="it">Gioco in cui i giocatori assumono il ruolo di personaggi in un ambiente immaginario</rdfs:comment>
    <rdfs:subClassOf rdf:resource="dbpedia:Game"/>
  </rdfs:Class>
 
  <rdfs:Class rdf:ID="PaperAndPencilGame">
    <!--<owl:equivalentClass rdf:resource="https://www.wikidata.org/wiki/Q2347716"/>-->
    <rdfs:label xml:lang="en">paper and pencil game</rdfs:label>
    <rdfs:label xml:lang="it">gioco di carta e matita</rdfs:label>
    <rdfs:comment xml:lang="it">Gioco in cui i giocatori assumono il ruolo di personaggi in un ambiente immaginario</rdfs:comment>
    <rdfs:subClassOf rdf:resource="dbpedia:Game"/>
  </rdfs:Class>
 
  <!-- Class Tabletop? Sinonimo di BoardGame -->
  <!--
  <rdfs:Class rdf:ID="TabletopGame">
    <owl:sameAs rdf:resource="http://dbpedia.org/ontology/BoardGame"/>
  </rdfs:Class>
  -->
 
  <rdfs:Class rdf:ID="DiceGame">
    <!--<owl:equivalentClass rdf:resource="https://www.wikidata.org/wiki/Q1515156"/>-->
    <rdfs:label xml:lang="en">dice game</rdfs:label>
    <rdfs:label xml:lang="it">gioco di dadi</rdfs:label>
    <rdfs:comment xml:lang="en">game in which dice are the central component</rdfs:comment>
    <rdfs:comment xml:lang="it">gioco basato sul lancio di dadi</rdfs:comment>
    <rdfs:subClassOf rdf:resource="dbpedia:Game"/>
  </rdfs:Class>
 
  <!-- TODO
    Class RolePlayingBoardGame extends BoardGame, RolePlayingGame
    Class PenAndPaperRolePlayingGame extends PenAndPaperGame, RolePlayingGame
    Dungeons and Dragons 

    REUSE
    dbpedia:BoardGame
    dbpedia:CardGame
  -->
  
  <rdfs:Class rdf:ID="Publisher">
    <rdfs:comment>The publisher of the game.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="dbpedia:Company"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Designer">
    <rdfs:comment>Any type of designer.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="foaf:Person"/>
  </rdfs:Class>

  <!-- or dbpedia:Artist -->
  <rdfs:Class rdf:ID="Artist">
    <rdfs:comment>Any type of artist.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="foaf:Person"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Shape">
    <rdfs:comment></rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:ID="2DShape">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Shape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="3DShape">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Shape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Circle">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#2DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Triangle">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#2DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Rectangle">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#2DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Square">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#2DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Sphere">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#3DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Pyramid">
    <rdfs:comment></rdfs:comment>
    <rdfs:comment>Parallelepipedo rettangolo</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#3DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="RectangularCuboid">
    <rdfs:comment></rdfs:comment>
    <rdfs:comment>Parallelepipedo rettangolo</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#3DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Cube">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#3DShape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Material">
    <rdfs:comment>Physical material.</rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Plastic">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Material"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Wood">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Material"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Paper">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Material"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Cardboard">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Material"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="GameComponent">
    <rdfs:comment>Generic piece of a game.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Shape"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Token">
    <rdfs:comment>Forma 2D con uno spessore</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#2DShape"/>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Board">
    <rdfs:comment>Forma 2D con uno spessore</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#2DShape"/>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Dice">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Cube"/>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Box">
    <rdfs:comment>Scatola del gioco. La tengo? Dovrei farla </rdfs:comment>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Book">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>
    <rdfs:subClassOf rdf:resource="foaf:Document"/>

    <!-- Paper -->
  </rdfs:Class>

  <rdfs:Class rdf:ID="Manual">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Book"/>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Card">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>

    <!-- Paper or plastic -->
  </rdfs:Class>

  <rdfs:Class rdf:ID="Deck">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>

    <!-- Contengono le carte -->
  </rdfs:Class>

  <rdfs:Class rdf:ID="Miniature">
    <rdfs:comment></rdfs:comment>
    <rdfs:subClassOf rdf:resource="#GameComponent"/>
  </rdfs:Class>

  <!-- Properties -->

  <!--
  TODO proprietà di un gioco
  ✓ età (range - min, max)
  ✓ Playing time/duration
  ✓ edizione

  // Esempio di sotto-proprietà https://boardgamegeek.com/boardgamecategory/1032/action-dexterity
  // https://boardgamegeek.com/boardgame/37111/battlestar-galactica-board-game
  Type
  Category
  Mechanisms
  ✓ Family

  // https://boardgamegeek.com/boardgame/194286/catacombs-castles
  // Per owl: se hanno queste proprietà -> hanno una famiglia in comune
  Reimplemented By
  Reimplements
  Integrates With

  premi - dbpedia:Award
  -->

  <!--
  TODO proprietà dei componenti
  Dado (✓ numeroDiFacce, tipoDiFacce) <- FormaGeometrica3D
  Miniatura (dimensioni?, FormaGeometrica2DBase, Descrizione)
  Shape3D (dimensioni?)
  Shape2D (dimensioni?)
  Plancia / Tabellone / Board (✓ spessore) <- FormaGeometrica2D
  Libro (✓ lingua, ✓ tipoCarta, ✓ numeroDiPagine, ✓ formato?, dimensione?)
  Manuale () <- Libro
  Carte (dimensioni, lingua?, tipoCarta)
  Token (spessore) <- FormaGeometrica2D
  ComponentiSpeciali Es. cappellino, occhiali, etc.
  -->

  <!--
  ✓ artista - dbpedia:Artist
  ✓ colore - dbpedia:Colour
  ✓ foto - dbpedia:Image
  ✓ lingua - dbpedia:Language
  ✓ libro - foaf:Document
  -->

  <rdf:Property rdf:ID="title">
    <rdfs:comment>The title of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="description">
    <rdfs:comment>The description of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="publicationDate">
    <rdfs:comment>The date of publication of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <!-- TODO dovrebbe essere un valore unico o una coppia (min-max) -->
  <rdf:Property rdf:ID="duration">
    <rdfs:comment>The duration of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <!-- TODO dovrebbe essere un valore unico o una coppia (min-max) -->
  <rdf:Property rdf:ID="age">
    <rdfs:comment>The minimum age to play the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <!-- TODO dovrebbe essere un valore unico o una coppia (min-max) -->
  <rdf:Property rdf:ID="numberOfPlayers">
    <rdfs:comment>The number of players of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <!-- FIXME il publisher è una persona o una compagnia, publisher è una proprietà di Game -->
  <rdf:Property rdf:ID="publisher">
    <rdfs:comment>The publisher of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="#GamePublisher"/>
  </rdf:Property>
  
  <!--
  <rdf:Property ID="minimum"
    comment="The minimum numeric value permitted."/>

  <rdf:Property ID="maximum"
    comment="The maximum numeric value permitted."/>
  -->

  <rdf:Property rdf:ID="edition">
    <rdfs:comment>The edition of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="family">
    <rdfs:comment>The family of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="award">
    <rdfs:comment>The award of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="dbpedia:Award"/>
  </rdf:Property>

  <rdf:Property rdf:ID="hasBox">
    <rdfs:comment>A box of the game.</rdfs:comment>
    <rdfs:domain rdf:resource="#Game"/>
    <rdfs:range rdf:resource="#Box"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="hasComponent">
    <rdfs:comment>A component of the box.</rdfs:comment>
    <rdfs:domain rdf:resource="#Box"/>
    <rdfs:range rdf:resource="#GameComponent"/>
  </rdf:Property>

  <rdf:Property rdf:ID="description">
    <rdfs:comment>The description of the component.</rdfs:comment>
    <rdfs:domain rdf:resource="#GameComponent"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="hasHeight">
    <rdfs:comment>Height, in millimeters.</rdfs:comment>
    <rdfs:domain rdf:resource="#GameComponent"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="hasWidth">
    <rdfs:comment>Width, in millimeters.</rdfs:comment>
    <rdfs:domain rdf:resource="#GameComponent"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="material">
    <rdfs:comment>Material used to make the component.</rdfs:comment>
    <rdfs:domain rdf:resource="#GameComponent"/>
    <rdfs:range rdf:resource="#Material"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="hasColour">
    <rdfs:comment>The main colour of the component.</rdfs:comment>
    <rdfs:domain rdf:resource="#GameComponent"/>
    <rdfs:range rdf:resource="#dbpedia:Colour"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="hasPhoto">
    <rdfs:comment>A photo of the component.</rdfs:comment>
    <rdfs:domain rdf:resource="#GameComponent"/>
    <rdfs:range rdf:resource="#dbpedia:Image"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="hasArtist">
    <rdfs:comment>The creator of the component.</rdfs:comment>
    <rdfs:domain rdf:resource="#GameComponent"/>
    <rdfs:range rdf:resource="#Artist"/>
  </rdf:Property>

  <rdf:Property rdf:ID="faces">
    <rdfs:comment>The number of faces of the dice.</rdfs:comment>
    <rdfs:domain rdf:resource="#Dice"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="hasBase">
    <rdfs:comment>The base of the miniature.</rdfs:comment>
    <rdfs:domain rdf:resource="#Miniature"/>
    <rdfs:range rdf:resource="#Shape2D"/>
  </rdf:Property>

  <rdf:Property rdf:ID="thickness">
    <rdfs:comment>The thickness of the board.</rdfs:comment>
    <rdfs:domain rdf:resource="#Board"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="thickness">
    <rdfs:comment>The thickness of the token.</rdfs:comment>
    <rdfs:domain rdf:resource="#Token"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>

  <rdf:Property rdf:ID="paperSize">
    <rdfs:comment>The paper size.</rdfs:comment>
    <rdfs:domain rdf:resource="#Paper"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="weight">
    <rdfs:comment>The weight/grammatura of the paper.</rdfs:comment>
    <rdfs:domain rdf:resource="#Paper"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="hasLanguage">
    <rdfs:comment>The language of the text of the board.</rdfs:comment>
    <rdfs:domain rdf:resource="#Board"/>
    <rdfs:range rdf:resource="dbpedia:Language"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="language">
    <rdfs:comment>The language of the text of the book.</rdfs:comment>
    <rdfs:domain rdf:resource="#Book"/>
    <rdfs:range rdf:resource="dbpedia:Language"/>
  </rdf:Property>
  
  <rdf:Property rdf:ID="pages">
    <rdfs:comment>The number of the pages of the book.</rdfs:comment>
    <rdfs:domain rdf:resource="#Book"/>
    <rdfs:range rdf:resource="rdfs:Literal"/>
  </rdf:Property>
  

  <!-- RDF -->



</rdf:RDF>
